{
  "plateReader": {
    "key": "plateReader",
    "name": "Plate Reader",
    "cellType": "PlateReader",
    "parseOptions": {
      "delimiter": ",",
      "startRow": 16,
      "columns": [
        "row",
        "column",
        "content",
        "id",
        "concentration"
      ],
      "metadata": {
        "rows": 3,
        "idColumn": "ID1",
        "delimiter": ","
      }
    },
    "conversion": {
      "factors": {
        "dilution": 500,
        "standardInsertSize": 452,
        "libraryInsertSize": 573
      },
      "expression": "(dilution*standardInsertSize)/libraryInsertSize"
    },
    "qcResults": {
      "key": "Molarity",
      "units": "nM",
      "assay": {
        "type": "Plate Reader",
        "version": "v1.0"
      }
    }
  },
  "qPCR": {
