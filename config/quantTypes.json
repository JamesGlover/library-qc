{
  "plateReader": {
    "key": "plateReader",
    "name": "Plate Reader",
    "cellType": "PlateReader",
    "parse": {
      "delimiter": ",",
      "rowDelimiter": ["\r\n", "\r", "\n"],
      "from": 16,
      "columns": [
        "row",
        "column",
        "content",
        "id",
        "concentration"
      ]
    },
    "metadata": {
      "rows": 3,
      "idColumn": "ID1",
      "delimiter": ","
    },
    "conversion": {
      "factors": {
        "dilution": 500,
        "standardInsertSize": 452,
        "libraryInsertSize": 573
      },
      "expression": "(dilution*standardInsertSize)/libraryInsertSize"
    },
    "qcResults": {
      "key": "Molarity",
      "units": "nM",
      "assay": {
        "type": "Plate Reader",
        "version": "v1.0"
      }
    }
  },
  "qPCR": {
    "key": "qPCR",
    "name": "qPCR",
    "cellType": "QPCR",
    "parse": {
      "delimiter": "\t",
      "rowDelimiter": ["\r\n", "\r", "\n"],
      "from": 3,
      "columns": [
        "include",
        "color",
        "pos",
        "name",
        "cp",
        "concentration",
        "standard",
        "status"
      ]
    },
    "metadata": {
      "rows": 1,
      "idColumn": "Experiment",
      "delimiter": "  "
    },
    "conversion": {
      "factors": {
        "averageLibrarySize": 585
      },
      "expression": "(1515.151*(1/averageLibrarySize))"
    },
    "qcResults": {
      "key": "Molarity",
      "units": "nM",
      "assay": {
        "type": "qPCR",
        "version": "v1.0"
      }
    }
  }
}